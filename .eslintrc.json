{
  "env": {
    "browser": true,
    "es6": true,
    "node": true
  },
  "extends": ["airbnb-base"],
  "globals": {
    "Atomics": "readonly",
    "SharedArrayBuffer": "readonly"
  },
  "parserOptions": {
    "ecmaVersion": 2018,
    "sourceType": "module"
  },
  "rules": {
    "consistent-return": "off",
    "eqeqeq": "off",
    "no-unexpected-multiline": "off",
    "no-shadow": "warn",
    "prefer-destructuring": "off",
    "no-param-reassign": "off",
    "array-callback-return": "warn"
  }
}
